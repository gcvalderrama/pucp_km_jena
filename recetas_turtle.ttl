###  http://www.recetario.com/de_origen

:de_origen rdf:type :ObjectProperty ;
           
           rdfs:domain :Receta ;
           
           rdfs:range :TipoReceta .



###  http://www.recetario.com/es_ingrediente_de

:es_ingrediente_de rdf:type :ObjectProperty ;
                   
                   rdfs:domain :Ingrediente ;
                   
                   rdfs:range :Receta .



###  http://www.recetario.com/nivelCalorias

:nivelCalorias rdf:type :ObjectProperty ;
               
               rdfs:range :NiveldeCalorias .



###  http://www.recetario.com/nivelSabor

:nivelSabor rdf:type :ObjectProperty .



###  http://www.recetario.com/tiene_ingrediente

:tiene_ingrediente rdf:type :ObjectProperty ,
                            :TransitiveProperty ;
                   
                   rdfs:range :Ingrediente ;
                   
                   rdfs:domain :Receta ;
                   
                   :inverseOf :es_ingrediente_de .





